    "league/tactician-container": "^2.0",
    "moon-php/http-middleware": "^3.0",
    "narrowspark/http-emitter": "^0.8.0",
    "phauthentic/correlation-id": "^1.0",
    "phauthentic/email": "dev-develop",
    "psr/http-client": "^1.0",
